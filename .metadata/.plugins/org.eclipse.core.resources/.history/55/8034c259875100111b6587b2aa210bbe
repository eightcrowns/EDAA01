package fractal;

import koch.Koch;
import mountain.*;

public class FractalApplication {
	public static void main(String[] args) {
		Fractal[] fractals = {
				new Koch(300),
				new Mountain(new Point(100, 200), new Point(200, 100), new Point(300, 200))};
	    new FractalView(fractals, "Fraktaler");
	}

}
